  "keywords": ["moodle", "travis", "ci", "testing"],
  "type": "project",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Mark Nielsen",
      "email": "mark@moodlerooms.com",
      "homepage": "http://moodlerooms.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "phing/phing": "2.*",
    "squizlabs/php_codesniffer": "2.3.*",
    "sebastian/phpcpd": "2.*",
    "phpmd/phpmd": "2.2.*",
    "symfony/filesystem": "2.*",
    "symfony/finder": "2.*",
    "symfony/console": "2.*",
    "pear/versioncontrol_git": "@dev"
